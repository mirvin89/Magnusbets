// Dark mode toggle

document.getElementById('dark-mode-toggle').addEventListener('click', function() {

    document.body.classList.toggle('dark-mode');

});

// ROI Calculator

document.getElementById('roi-form').addEventListener('submit', function(e) {

    e.preventDefault();

    const stake = parseFloat(document.getElementById('stake').value);

    const odds = parseFloat(document.getElementById('odds').value);

    if (isNaN(stake) || isNaN(odds) || stake <= 0 || odds <= 1) {

        document.getElementById('roi-result').innerHTML = '<p>Please enter valid stake and odds.</p>';

        return;

    }

    const payout = stake * odds;

    const roi = ((payout - stake) / stake) * 100;

    document.getElementById('roi-result').innerHTML = `<p>Payout: $${payout.toFixed(2)}, ROI: ${roi.toFixed(2)}%</p>`;

});

// Load games from JSON

fetch('data.json')

.then(response => response.json())

.then(data => {

    const games = data.games;

    const gamesContainer = document.getElementById('games-cards');

    const spreadsContainer = document.getElementById('spreads-cards');

    const totalsContainer = document.getElementById('totals-cards');

    games.forEach(game => {

        // Games card

        const gameCard = document.createElement('div');

        gameCard.className = 'card';

        gameCard.innerHTML = `<p><strong>${game.team1} vs ${game.team2}</strong></p><p>Date: ${game.date}</p>`;

        gamesContainer.appendChild(gameCard);

        // Spread card

        const spreadCard = document.createElement('div');

        spreadCard.className = 'card';

        spreadCard.innerHTML = `<p><strong>${game.team1} vs ${game.team2}</strong></p><p>Spread: ${game.spread}</p>`;

        spreadsContainer.appendChild(spreadCard);

        // Total card

        const totalCard = document.createElement('div');

        totalCard.className = 'card';

        totalCard.innerHTML = `<p><strong>${game.team1} vs ${game.team2}</strong></p><p>Total: ${game.total}</p>`;

        totalsContainer.appendChild(totalCard);

    });

})

// Load historical from CSV

fetch('data.csv')

.then(response => response.text())

.then(text => {

    const lines = text.trim().split('\n');

    const header = lines.shift(); // remove header

    const historicalContainer = document.getElementById('historical-data');

    lines.forEach(line => {

        const [game, odds, result] = line.split(',');

        const p = document.createElement('p');

        p.textContent = `${game}: Odds ${odds}, Result: ${result}`;

        historicalContainer.appendChild(p);

    });

});

